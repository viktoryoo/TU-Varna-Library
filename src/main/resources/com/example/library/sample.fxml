<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox alignment="CENTER" prefHeight="292.0" prefWidth="292.0" spacing="20.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/17.0.2-ea" fx:controller="com.example.library.controllers.Controller">
  <padding>
    <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
  </padding>
   <Label text="Email" />
   <TextField fx:id="username"/>
   <Label text="Password" />
   <PasswordField fx:id="password"/>

  <Label fx:id="welcomeText" />
  <Button onAction="#onLoginButtonClick" text="Login" />
</VBox>
